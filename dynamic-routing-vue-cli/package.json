{
  "name": "dynamic-routing-vue-cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "//": "手动运行命令: npm run dev --project=project-a",
    "dev": "cross-env PROJECT=$npm_config_project vue-cli-service serve",
    "build": "cross-env PROJECT=$npm_config_project vue-cli-service build",
    "//cli": "交互式命令: npm start",
    "start": "node cli.mjs",
    "start:dev": "node cli.mjs dev",
    "start:build": "node cli.mjs build"
  },
  "dependencies": {
    "inquirer": "^9.1.4",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "cross-env": "^7.0.3",
    "typescript": "~4.5.5"
  }
}
